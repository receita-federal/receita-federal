    <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f7f9fc;
      margin: 0;
      padding: 0;
      color: #333;
    }
    header {
      background-color: #007a33;
      color: white;
      padding: 20px;
      text-align: center;
    }
    main {
      display: flex;
      max-width: 1000px;
      margin: 40px auto;
      background: white;
      border-radius: 8px;
      box-shadow: 0 4px 10px rgba(0,0,0,0.1);
      padding: 30px;
      gap: 40px; /* espace entre photo et formulaire */
    }
    .photo-container {
      flex: 2; /* prend plus de place */
      display: flex;
      justify-content: center;
      align-items: center;
    }
    .photo-container img {
      max-width: 100%;
      height: auto;
      border-radius: 8px;
    }
    .form-container {
      flex: 1; /* moins large */
      display: flex;
      flex-direction: column;
      justify-content: center;
    }
    form label {
      display: block;
      margin-bottom: 6px;
      font-weight: bold;
      font-size: 0.9em;
    }
    form input,
    form select {
      width: 100%;
      padding: 8px 10px;
      margin-bottom: 15px;
      border: 1px solid #ccc;
      border-radius: 4px;
      font-size: 1em;
      box-sizing: border-box;
    }
    form button {
      background-color: #28a745;
      color: white;
      border: none;
      width: 100%;
      padding: 12px;
      font-size: 1.1em;
      border-radius: 4px;
      cursor: pointer;
      transition: background-color 0.3s ease;
    }
    form button:hover {
      background-color: #218838;
    }
    small.note {
      font-size: 0.8em;
      color: #666;
      margin-bottom: 15px;
      display: block;
      text-align: center;
    }
  </style>
